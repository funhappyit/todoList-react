;\nfunction createBulkTodos() {\n  const array = [];\n  for (let i = 1; i <= 2500; i++) {\n    array.push({\n      id: i,\n      text: `할 일${i}`,\n      checked: false,\n    });\n  }\n  return array;\n}\nconst App = () => {\n  const [todos, setTodos] = useState(createBulkTodos);\n  //추가\n  //고윳값으로 사용될 id\n  //ref를 사용하여 변수 담기\n  const nextId = useRef(2501);\n  const onInsert = useCallback(\n    (text) => {\n      const todo = {\n        id: nextId.current,\n        text,\n        checked: false,\n      };\n      setTodos((todos) => todos.concat(todo));\n      nextId.current += 1; //nextId 1씩 더하기\n    },\n    [todos],\n  );\n  //삭제\n  const onRemove = useCallback(\n    (id) => {\n      setTodos((todos) => todos.filter((todo) => todo.id !== id));\n    },\n    [todos],\n  );\n  //수정\n  const onToggle = useCallback(\n    (id) => {\n      setTodos((todos) =>\n        todos.map((todo) =>\n          todo.id === id ? { ...todo, checked: !todo.checked } : todo,\n        ),\n      );\n    },\n    [todos],\n  );\n  return (\n    <TodoTemplate>\n      <TodoInsert onInsert={onInsert} />\n      <TodoList todos={todos} onRemove={onRemove} onToggle={onToggle} />\n    </TodoTemplate>\n  );\n};\n\nexport default App;\n","C:\\Users\\user\\todo-app\\src\\TodoTemplate.js",[],"C:\\Users\\user\\todo-app\\src\\TodoInsert.js",[],"C:\\Users\\user\\todo-app\\src\\TodoListItem.js",[],"C:\\Users\\user\\todo-app\\src\\index.js",[],"C:\\Users\\user\\todo-app\\src\\TodoList.js",[],{"ruleId":"35","severity":1,"message":"36","line":1,"column":8,"nodeType":"37","messageId":"38","endLine":1,"endColumn":12},{"ruleId":"39","severity":1,"message":"40","line":34,"column":5,"nodeType":"41","endLine":34,"endColumn":12,"suggestions":"42"},{"ruleId":"39","severity":1,"message":"40","line":41,"column":5,"nodeType":"41","endLine":41,"endColumn":12,"suggestions":"43"},{"ruleId":"39","severity":1,"message":"40","line":52,"column":5,"nodeType":"41","endLine":52,"endColumn":12,"suggestions":"44"},"no-unused-vars","'